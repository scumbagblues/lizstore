{
    "version": 2,
    "regions": [
        "fra1"
    ],
    "builds": [
        {
            "src": "index.php",
            "use": "@vercel/php"
        }
    ],
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.1"
        }
    },
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/build/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "outputDirectory": "public"
}