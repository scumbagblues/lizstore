{
    "version": 2,
    "regions": [
        "fra1"
    ],
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.1"
        }
    },
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/build/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "commands": {
            "predeploy": "mkdir -p bootstrap/cache && chmod -R 775 bootstrap/cache"
    },
    "outputDirectory": "public"
}